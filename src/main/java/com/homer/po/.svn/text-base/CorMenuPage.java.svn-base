	static final By ProgramSetupReport=By.xpath("//*[@id='corAccordionProgramSetupReport']/a");
	
	public CorMenuPage(InstanceContainer ic) {
		 super(ic);       	
	}
	
	public void chooseProgramSetUp(){
		try{
			
			wh.clickElement(ProgramSetUp);
			
		}catch (Exception ex) {
			System.out.println(ex.getMessage());
			wh.handleAlert();
		}
		
		}
	
