package com.homer.po;

import org.openqa.selenium.By;

import com.homer.dao.InstanceContainer;

public class CorMenuPage extends PageBase<CorMenuPage> {
	
	static final By ProgramSetUp=By.xpath("//*[@id='accordion']/div[2]/div[1]/h4/a");
	static final By CommodityOnBoarding=By.xpath("//*[@id='corAccordionCob']/a");
	static final By MasterProgramSetUp=By.xpath("//*[@id='accordion']/div[2]/div[1]/h4/a");
